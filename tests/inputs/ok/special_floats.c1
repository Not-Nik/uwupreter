void main() {
    float pos_inf = 1.0 / 0;
    float neg_inf = -1.0 / 0;
    float nan = 0.0 / 0;

    print(pos_inf); // inf
    print(neg_inf); // -inf
    print(nan);     // nan

    print(nan == nan);  // false
    print(nan != nan);  // true
    print(pos_inf + 1); // inf
    print(neg_inf - 1); // -inf
}
