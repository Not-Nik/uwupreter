int ackermann(int m, int n) {
    if (m == 0)
        return n + 1;

    if (n == 0)
        return ackermann(m - 1, 1);

    return ackermann(m - 1, ackermann(m, n - 1));
}

void main() {
    for (int m = 0; m <= 3; m = m + 1) {
        for (int n = 0; n < 6 - m; n = n + 1) {
            print("---");
            print(m);
            print(n);
            print(ackermann(m, n));
        }
    }
}
